*, *::before, *::after{
  box-sizing: border-box;
  margin: 0px;
  padding: 0px;
}



// Definiendo Menu para tamaño Web

.menuWeb{
  height 120px
  display flex
  justify-content: space-between;
  align-items center
  display none

  position relative
}

.menuWeb ul{
  list-style: none
  display flex
  justify-content center
  align-items center
  height 40px
}

.menuWeb ul a{
  text-decoration none
  color letras_negras
  font-family:  Menu_mobile
}

.menuWeb .navigation ul #variedades{
  color verde
}


.navigation{
  text-align right
  font-size Tamano_mensaje_menu
  color verde
  font-family:  Menu_mobile
}
.pulsaWeb{
  width  auto
  height 25px
  display flex
  align-items center
  justify-content flex-end
}
.center{
  border-radius 5px
  display flex
  align-items center
  justify-content center
  margin 0 10px


}
.rigth{
  text-align right
}

#insumos{
  background verde
  width 120px
  color letras_blancas
//  font-family: Menu_desplegable
}
#variedades{
  background Insumos_menu_mobile
  width 120px
//  font-family: Menu_desplegable
}




// Definiendo los tamaños del documento

.box-uno{
z-index 20
}
header{
  position relative
  z-index: 0
  height: Altura_Banner;
  background-image: url(../images/portada.jpg);
  background-size: cover;
  background-repeat: no-repeat;
  border: 1px solid transparent;
  opacity: 0.8;
}

// Margen de todas las secciones del documento

.container{
  margin: 0 10px;
}

// Logotipo superior del banner
.logo{
  margin: -10px auto 10px;
  text-align: center;
}

//mensaje del banner

.mensaje{
  margin: 40px auto 10px;
  color: letras_blancas;
  font-size: Tamano_mensaje_banner;
  text-align: center;
  font-family: Banner;
}

//Cajas de menu mobile

[class^="box-"]{
  color: letras_blancas;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}

.nav{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top 10px
}

.box-uno a{
  color: letras_blancas;
  text-decoration: none;
  font-size: Tamano_mensaje_burguer_banner;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  cursor: pointer;
}
.bar{
  display block
  position fixed
}

.box-dos a{
  font-family:  Lenguaje;
  font-size: Tamano_mensaje_Lenguaje_banner;
  letter-spacing: 2pt;
  cursor: pointer;
}

// Definiendo menu de navegación mobile


.menu{
  width: Anchura_Menu_Desplegable;
  height: Altura_Menu_Desplegable;
  background: negro_menu_mobile;
  display: flex;
  flex-wrap: wrap;
  justify-content: center ;
  align-items: center;
  box-shadow: 5px 0 5px black;
  position: fixed
  top: 0;
  left: -100vw;
  transition .5s
  z-index: 10
}
.menu.is-active{
left 0
}

.menu ul{
  list-style: none;
  margin-top: 70px;
  width: 200px;
  height: 70%;
  display: flex;
  flex-wrap: wrap;
  justify-content: center ;
  align-items: center;
}

.menu ul li a{
  text-decoration: none;
  color: rgb(255, 255, 255);
}

.pulsa{
  width: 150px;
  height: 40px;
  margin: auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: center ;
  align-items: center;
  font-family Menu_mobile
}
.pulsa a{
  cursor pointer
  outline: none
}

.botonUno{
  border-radius: 5px;
  font-family: Menu_desplegable
  letter-spacing:  2pt
  background: Variedades_verde_menu_mobile;
  font-size .9em
  cursor pointer
  outline: none
}

.botonDos{
  background: Insumos_menu_mobile;
  border-radius: 5px;
  font-family: Menu_desplegable
  letter-spacing:  2pt
  font-size .9em
  cursor pointer
  outline: none
}

.fondoNav{
  width: 30px;
  height: 30px;
  background-color: Blanco_para_logos;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  margin: 0 7px;
  cursor pointer
  outline: none
}

.fondoNav a{
  text-decoration: none;
  display: flex;
  align-items: center;
}

.fondoNav a span{
  color: negro_menu_mobile;
}


// Sección de conoce nuestras VARIEDADES

#tol{
display none
}

.slideUno{
  height: Altura_Nuestras_Variedades;
  width 100%
  position relative
  z-index: 0
}

.fondoSlide{
  height: Altura_Nuestras_Variedades;
  width 100%
  background-image: url(../images/portada-variedades.jpg);
  background-size: cover;
  background-repeat: no-repeat;
  opacity: .2;
  position relative
}

.contenido{
  position absolute
  top 0
  left 0
  bottom 0
  display: block;
  text-align: center;
  width 100%
}
.cabeza{
position relative
height 20%
display flex
justify-content center
align-items center
}
.cabezaDos{
  height 80%
  display flex
  justify-content: space-around;
  align-items center
}

.cabezaDos li{
  list-style none
}
.caja{
display flex
justify-content: space-between;
align-items center
height 200px
width 310px
}

[class^="img-"]{
  margin: 10px;
  display: flex;
  align-items: center;
  width: 130px;
  height: 130px;
  cursor: pointer;
}

.square{

  width: 90%;
  height: 90%;
  margin: auto;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(0, 0, 0, 0);
}
.square:hover{
  background-color: rgba(0, 0, 0, 0.5);
  border: 1px solid white;
}

.img-1{
  background-image: url(../images/r001.jpg);
  background-size: cover;
  background-repeat: no-repeat;
}
.img-2{
  background-image: url(../images/r002.jpg);
  background-size: cover;
  background-repeat: no-repeat;
}

.img-3{
  background-image: url(../images/r003.jpg);
  background-size: cover;
  background-repeat: no-repeat;
}
.img-4{
  background-image: url(../images/r004.jpg);
  background-size: cover;
  background-repeat: no-repeat;
}

.btn{
  color: transparent;
  background-color: transparent;
  border none
  outline: none;
  width: 70%;
  height: 25px;
  font-size: .7em;
}
.btn:hover{
  color: white;
  background-color: transparent;
  border: 1px solid white;
  outline: none;
  width: 70%;
  height: 25px;
  font-size: .7em;
}

// Sección de conoce nuestras Nuestra filosofia

.description{
  position relative
  z-index: 0
  height: Altura_Nuestras_Filosofia ;
  background-image: url(../images/insumos.jpg);
  background-size: cover;
  background-repeat: no-repeat;
  border: 1px solid transparent;
  opacity: 0.8;
}

.cajita{
  width: 280px;
  margin: 25px 0;
}

.tituloTres
  color: letras_blancas;
  font-size: Tamano_nuestra_filosofia;
  font-weight: 400;
  width: 100%;
  text-align: left;
  margin: 20px 0 0;
  font-family: Banner;

.text{
  font-size: Tamano_nuestra_filosofia_texto;
  color: letras_blancas;
  font-family: Lenguaje;
  line-height: 10.8pt;
  margin: 20px 0 10px;
  width: 250px;
  height: 70px
}
.cajita button{
  background: transparent;
  color: letras_blancas;
  width: 100px;
  height: 25px;
  border: 1px solid Blanco_para_logos;
  font-size: Tamano_nuestra_filosofia_Botones;
  font-family: Menu_desplegable
  cursor pointer
  outline: none
}


// Seccion  dedicada a la publicaciones y eventos

#changueDos{
display none
}


.slideDos{
  height: Altura_publicaciones_eventos
  background-color: letras_blancas;
  width 100%
  margin 0
  position relative
  display block
  z-index: 0
}

.tituloDos{
  font-family: Titulos_slides;
  text-align center
  margin 10px 0
  font-size Tamano_Titulo_seccion
}
.data{
  text-align: left;
  margin: 0;
}

.imgSlide{
  width: 100%;
  margin: 0 auto;
}
.data h4{
  font-size: Tamano_Titulo_imagen_publicaciones_eventos;
  color: verde;
  margin: 5px auto 0;
  font-family: Menu_desplegable;
}
.data p{
  font-size: Tamano_Texto_imagen_publicaciones_eventos;
  width: 240px;
  margin: 5px 0 0;
  font-family: Menu_desplegable;
}
.data button{
  background:verde;
  border: none;
  border-radius: 5px;
  width: 100px;
  height: 25px;
  font-size: 7pt;
  font-family: Lenguaje;
  margin: 10px 0 0;
  color: letras_blancas;
  font-family: Menu_desplegable;
  outline: none
  cursor pointer
}




// slider publicaciones y eventos


.boxUno{
  height: 80px;
  padding: 20px;
}

.flexslider{
  margin: auto 0px;
  height: 80%;
}

.boxDos{
  list-style: none;
  height: 230px;
}

.flex-control-nav.flex-control-paging{
	width: 150px;
	margin: 20px auto 20px;
	display: flex;
	justify-content: center;
	align-items: center;
	align-self: center;
	list-style: none;

	z-index: 3;
}
.flex-control-nav li{
	display: inline-block;
	margin: 0 5px;
}
.flex-control-nav li a{
	display: block;
	width: 15px;
	height: 15px;
	background: letras_blancas;
  border: 1px solid verde;
	border-radius: 50%;
	text-indent: -9999px;
}
.flex-control-nav li a.flex-active{
	background: verde;
}

.flex-direction-nav{
z-index: 3;
list-style: none;
}



// Contenido dedicado al FOOTER

footer{
  border: 1px solid transparent;
  background-color: verde;
  position relative
  z-index: 0
}
.contenedor{
  width: 280px;
  margin: 5px auto;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.footer{
  display: flex;
  flex-wrap: wrap;
}
.contenedor img{
  width: 100%;
}
.contacto{
  width: 250px;
  margin: 5px auto;
  padding: 10px;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.contacto .caja-1{
  display: flex;
  align-items: center;
}
.contacto .caja-2{
  font-size: 9pt;
  width: 180px;
  display: flex;
  align-items: center;
}
.fondo{
  width: 30px;
  height: 30px;
  background-color: rgba(255, 255, 255, .5);
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  margin: 0 7px;
}
.fondo a{
  text-decoration: none;
  display: flex;
  align-items: center;
}
.fondo a span{
  color: #9ac488;
}
.caja-2 p{
  color: black;
  font-family: Lenguaje;
}
.contenedor .contactoDos{
  width: 250px;
  margin: 5px auto;
  padding: 10px;
  text-align: center;
  color: black;
  font-family: Lenguaje;
}
.socialMedia{
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
}
.contactoDos p{
  margin: 0 0 30px;
  font-size: 9pt;
}
.pieDePagina{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  height: 50px;
  background-color: black;
  align-items: center;
}
.pieDePagina a{
  text-decoration: none;
  color: verde;
  font-size: 9pt;
  margin: 8px;
  font-family: footer;
}
